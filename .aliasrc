#!/bin/sh

# Alias script for Víctor Pérez Cano

alias ls="colorls -l --sd"
alias la="ls -a"
alias grep="grep --color=auto"
alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias valgrind="valgrind --leak-check=full --track-origins=yes --show-leak-kinds=all -v"
alias vim="nvim"
alias mutt="neomutt"
alias waterfox="waterfox-current"
alias ex="extract"
alias cz="compress -zip"
alias cr="compress -rar"

