#!/bin/sh

# Alias script for Víctor Pérez Cano

alias ls="exa -l --icons --group-directories-first"
alias tree="exa -T"
alias la="ls -a"
alias grep="grep --color=auto"
alias valgrind="valgrind --leak-check=full --track-origins=yes --show-leak-kinds=all -v"
alias ex="extract"
alias cz="compress -zip"
alias cr="compress -rar"
alias vim="nvim"
alias em="devour emacsclient -c"
alias chrome="google-chrome-stable"
alias cat="bat"
alias cls="clear"
